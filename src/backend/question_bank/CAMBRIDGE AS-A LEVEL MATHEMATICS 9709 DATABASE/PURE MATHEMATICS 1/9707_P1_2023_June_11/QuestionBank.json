        ]
    },
    "QuestionContent": {
        "Text": "Solve the equation \\(4 \\sin \\theta + \\tan \\theta = 0\\) for \\(0^\\circ < \\theta < 180^\\circ\\)."
    },
    "SolutionContent": {
        "Steps": [
            {
                "Step": "Rewrite \\(\\tan \\theta\\) as \\(\\frac{\\sin \\theta}{\\cos \\theta}\\).",
                "Expression": "\\(4 \\sin \\theta + \\frac{\\sin \\theta}{\\cos \\theta} = 0\\)"
            },
            {
                "Step": "Factorize \\(\\sin \\theta\\):",
                "Expression": "\\(\\sin \\theta (4 \\cos \\theta + 1) = 0\\)"
            },
            {
                "Step": "Solve \\(\\sin \\theta = 0\\):",
                "Result": "No solutions within \\(0^\\circ < \\theta < 180^\\circ\\)."
            },
            {
                "Step": "Solve \\(4 \\cos \\theta + 1 = 0\\):",
                "Expression": "\\(\\cos \\theta = -\\frac{1}{4}\\)"
            },
            {
                "Step": "Find \\(\\theta\\) using inverse cosine:",
                "Expression": "\\(\\theta = \\cos^{-1}(-\\frac{1}{4})\\)",
                "Result": "\\(\\theta = 104.5^\\circ\\) (correct to 1 decimal place)."
            }
        ],
        "FinalAnswer": "\\(\\theta = 104.5^\\circ\\)",
        "MarkingScheme": {
            "M1": "Correct substitution for \\(\\tan \\theta\\) and factorization.",
            "M1": "Correct use of trigonometric identities or equations.",
            "A1": "Correct solution for \\(\\theta\\) to 1 decimal place."
        }
    }
},
