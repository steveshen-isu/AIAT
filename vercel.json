{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        },
        {
            "src": "src/backend/server.js",
            "use": "@vercel/node"
        },
        {
            "tmp/plot.py": {
                "runtime": "vercel-python@latest"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/build/$1"
        }
    ]
}