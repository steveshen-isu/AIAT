{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static",
        "config": {
          "distDir": "build"
        },
        "buildCommand": "npm run build"
      },
      {
        "src": "src/backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
  }